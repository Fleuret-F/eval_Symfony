{% extends 'base.html.twig' %}

{% block title %}Categorie index{% endblock %}

{% block body %}
<h1>Categorie index</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nom</th>
        </tr>
    </thead>
    <tbody>
        {% for categorie in categories %}
        <tr>
            <td>
                <a href="">{{ categorie.nom }}</a>
            </td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}